package com.statement.condition;

public class SwitchExample {
	public static void main(String[] args) {
		String id = "990101-1012345";
		int genderNum = id.indexOf(7);
		
		switch (genderNum % 2) {
			case 0 : 
				System.out.println("남성"); 
				break;
			case 1 : 
				System.out.println("여성");
				break;
		}
		
	}
}
