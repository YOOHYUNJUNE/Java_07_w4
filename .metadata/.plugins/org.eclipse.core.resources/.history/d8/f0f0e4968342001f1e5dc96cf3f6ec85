package com.oop.practice5.library.book;

public class EBook extends Book{

	String title;
	String name;
	String ISBN;
	int BookCount = 0;
	
	public Book() {}
	
	public Book(String title, String name, String isbn) {
		super();
		this.title = title;
		this.name = name;
		ISBN = isbn;
		BookCount++;
	}
	
	
	
	
	
	
	
	
	
	
}
