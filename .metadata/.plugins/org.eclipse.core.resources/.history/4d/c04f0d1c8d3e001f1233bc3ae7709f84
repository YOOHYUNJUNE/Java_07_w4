package com.reference.string;


public class StringPractice {
	public static void main(String[] args) {
		String address = "cik@java.co.kr";
		String userId = address.substring(0, 2);
		String companyName = address.substring(4, 7);
		String domainAddr = address.substring(4, 7);
		
		System.out.println("userId : " + userId);
		System.out.println("companyName : " + companyName);
		System.out.println("domainAddr : " + domainAddr);
		
		
	}
		
}

