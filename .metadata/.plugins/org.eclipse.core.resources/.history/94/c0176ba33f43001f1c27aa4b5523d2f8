package com.practice.mobile;

public class MobileExample {
	public static void main(String[] args) {
		
		Mobile l = new Ltab();
		Mobile o = new Otab();
		
		l.operate(10);
		System.out.println(l.batterySize);
		l.charge(20);
		System.out.println(l.batterySize);
		
		System.out.println();
		
		o.operate(10);
		System.out.println(o.batterySize);
		o.charge(20);
		System.out.println(o.batterySize);
		
		
		
	}
	
	
	
}
