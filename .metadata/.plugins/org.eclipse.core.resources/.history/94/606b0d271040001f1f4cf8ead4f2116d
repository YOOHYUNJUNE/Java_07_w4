package com.oop.basic.ch08;

public class BookExample {
	public static void main(String[] args) {
		
		Book A = new Book("SQL PLUS", 50000, 5.0);
		Book B = new Book("Java 2.0", 40000, 3.0);
		Book C = new Book("JSP Servlet", 60000, 6.0);
		
		
		System.out.println("책이름 \t\t 가격 \t 할인율 \t 할인후 금액");
		System.out.println("-------------------------------------------------");
		
		System.out.println(A.bookName + "\t" + A.bookPrice + " 원\t" + A.bookDiscountRate + "%\t" + A.getDiscoutBookPrice() + " 원");
		System.out.println(B.bookName + "\t" + B.bookPrice + " 원\t" + B.bookDiscountRate + "%\t" + B.getDiscoutBookPrice() + " 원");
		System.out.println(C.bookName + "\t" + C.bookPrice + " 원\t" + C.bookDiscountRate + "%\t" + C.getDiscoutBookPrice() + " 원");
		
		
		
	}
}
