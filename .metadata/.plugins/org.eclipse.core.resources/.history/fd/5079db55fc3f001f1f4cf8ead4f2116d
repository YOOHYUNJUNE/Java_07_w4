package com.oop.basic.test;

public class Student {
	// 과목 평균, 학점
	String name;
	int korean;
	int english;
	int math;
	int science;
	
	Student(String name, int korean, int english, int math, int science) {
		this.name = name;
		this.korean = korean;
		this.english = english;
		this.math = math;
		this.science = science;
	}
	
	public class StudentExample {
		// 합
		int sum(int ...values) {
			int result = 0;
			for (int i : values) {
				result += i;
			}
			return result;
		}
		
		// 평균
		double avg(double ...values) {
			double avg = 0;
			for (double j : values) {
				avg = (double) sum / 5;
			}
		}
		
		
		
		
	}
	
	
	
	
	
	
	
	
	
}
