package a;

// public 제거 시 : 타 패키지에서 호출 불가능
public class A {
	
	byte byteValue = 1;
	short shortValue = 1;
	int intValue = 1;
	
	// 생성자 접근제한
	ClassName cn1 = new ClassName(byteValue);
	ClassName cn2 = new ClassName(shortValue); // default : 같은 패키지에서만 가능
//	ClassName cn3 = new ClassName(intValue); // private : 같은 객체에서만 가능
	
	
}
