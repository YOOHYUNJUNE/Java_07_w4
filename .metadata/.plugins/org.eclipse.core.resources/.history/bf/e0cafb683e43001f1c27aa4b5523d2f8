package com.practice.mobile;

public class Ltab extends Mobile {
	
	public Ltab() {}
	
	public Ltab(String mobileName, String osType, int batterySize) {
		super(mobileName, osType, batterySize);
	}
	
	int battery;
	

	@Override
	int operate(int time) {
		return getBatterySize(battery - 8 * time);
	}

	@Override
	int charge(int time) {
		return getBatterySize() + time;
	}

}
