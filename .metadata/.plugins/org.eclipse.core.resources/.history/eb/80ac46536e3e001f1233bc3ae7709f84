package com.statement.loop;

public class BreakExample2 {
	public static void main(String[] args) {
		for (char upper='가'; upper <= '갸'; upper++) {
			for (char lower='a'; lower <= 'z'; lower++) {
				System.out.println(upper + " - " + lower);
				if (lower == 'g') {
					break;
				}
			}
		}
	}
}
