package com.statement.loop;

public class Test {
	class Solution {
	    public int solution(int n) {
	        int answer = 0;
	        for (n=1; n<=1000; n++) {
	            if (n % 2 == 0) {
	                answer += n;
	            } 
	        }
	       
	        
	        return answer;
	    }
	}
}
