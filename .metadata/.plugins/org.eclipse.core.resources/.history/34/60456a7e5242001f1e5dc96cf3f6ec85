package com.mycompany;

import com.hankook.Tire;
//import com.hankook.*;

public class Car {

	String brand, model;
	int speed;
	final int MAX_SPEED;
	Tire tire = new Tire();
	
	public Car(String brand, String model, int speed, int MAX_SPEED) {
		this.brand = brand;
		this.model = model;
		this.speed = speed;
		this.MAX_SPEED = MAX_SPEED;
	}
	
	
	
	
	
	
	
}
