package com.oop.practice5.library.book;

public class EBook extends Book{

	String title;
	String name;
	String ISBN;
	double fileSize;
	
	int BookCount = 0;
	
	public EBook() {}
	
	public EBook(String title, String name, String isbn, double size) {
		super();
		this.title = title;
		this.name = name;
		ISBN = isbn;
		fileSize = size;
//		BookCount++;
	}
	
	
	
	
	
	
	
	
	
	
}
