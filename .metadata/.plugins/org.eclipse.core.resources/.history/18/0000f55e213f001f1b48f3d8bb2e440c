package com.reference.list;

public class ListPractice {
	public static void main(String[] args) {
		String wetherArr[] = {"봄", "여름", "가을", "겨울"};
		wetherArr[0] = "Spring";
		wetherArr[1] = "Summer";
		wetherArr[2] = "Autumn";
		wetherArr[3] = "Winter";

		System.out.println(wetherArr[0]);
		System.out.println(wetherArr[1]);
		System.out.println(wetherArr[2]);
		System.out.println(wetherArr[3]);
		
		for (int i = 0; i < wetherArr.length; i++) {
			String wether = wetherArr[i];
			System.out.print(wether + " ");
		}
	}
}
