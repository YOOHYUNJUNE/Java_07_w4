package com.referencere.basic;

public class ReferenceExample {
	public static void main(String[] args) {
		String name = "유현준";
		String noName = null;
		
		// null 값인 변수는 참조 데이터 타입이어도 비교 연산자로 비교 가능
		System.out.println(name == noName);
		System.out.println(name != noName);
		
		// 참조 데이터 타입에서 가장 많이 발생하는 Exception은 NullPointerException
		// null 상태에서 접근 시 발생
		System.out.println(noName.charAt(0));

		
	}
}
