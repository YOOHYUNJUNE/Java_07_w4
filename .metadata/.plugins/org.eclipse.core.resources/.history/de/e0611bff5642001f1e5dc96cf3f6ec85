package a;

public class ClassName {
	byte byteValue;
	short shortValue;
	int intValue;
	
	public ClassName(byte byteValue) {
		this.byteValue = byteValue;
	}
	
	// 생성자 접근제한
	ClassName(short shortValue) {
		this.shortValue = shortValue;
	}
	
	private ClassName(int intValue) {
		this.intValue = intValue;
	}
	
	
	
	
}
