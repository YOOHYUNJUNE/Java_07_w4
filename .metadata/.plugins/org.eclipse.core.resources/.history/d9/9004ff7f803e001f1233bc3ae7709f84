package com.statement.practice;

public class Practice6 {
	public static void main(String[] args) {
		for (int i=1; i<=100; i++) {
			System.out.println(i);
			if (i % 3 == 0) {
				System.out.println(" three");
			} if (i % 5 == 0) {
				System.out.println(" five");
			} if (i % 7 == 0) {
				System.out.println(" seven");
			}
		}
	}
}





